			"Sancti\\Factories\\": "database/factories/",
			"Sancti\\Migrations\\": "database/migrations/",
			"Sancti\\Seeders\\": "database/seeders/"
		}
	},
	"authors": [{
		"name": "Marcin ≈Åukaszewski",
		"email": "marcin@breakermind.com"
	}],
